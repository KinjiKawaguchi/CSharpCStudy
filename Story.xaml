<Page x:Class="CStudy.Story"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CStudy"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
      Background="White"
      Title="Page2">
    <Grid>
        <WebBrowser x:Name="WB_Paiza" Source="https://paiza.io/ja/projects/new" Width="952" Margin="0,0,968,0"/>
        <TextBlock x:Name="TextBlock_Mail" HorizontalAlignment="Left" Margin="952,0,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Center" Height="1080" Width="968" FontSize="20" Foreground="#FF1FFF00" Background="Black"/>
        <Button x:Name="Button_Reply" Content="Reply" HorizontalAlignment="Left" Margin="1845,1048,0,0" VerticalAlignment="Top" Click="Button_Reply_Click"/>
        <TextBox x:Name="TextBox_Reply" HorizontalAlignment="Left" Margin="1017,1048,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="823" Foreground="#FF2BFF00" SelectionBrush="#FF00FF10" Background="#FF696262"/>
        <Button x:Name="Button_Retry" Content="Retry" HorizontalAlignment="Left" Margin="1845,1048,0,0" VerticalAlignment="Top" Click="Button_Try_Click" Cursor="Hand" Visibility="Hidden"/>
        <Button x:Name="Button_NextStory" Content="Next" HorizontalAlignment="Left" Margin="1845,1048,0,0" VerticalAlignment="Top" Click="Button_Try_Click" Visibility="Hidden" Cursor="Hand"/>

    </Grid>
</Page>